{
  "ignorePatterns": [
    {
      "pattern": "^(?!http).*"
    },
    {
      "pattern": "!\\[.*\\]\\((?!http).*\\)"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["*"],
      "headers": {
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/122.0.0.0 Safari/537.36"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 10,
  "fallbackRetryDelay": "1000s",
  "aliveStatusCodes": [
    0,
    200,
    401,
    403
  ]
}
