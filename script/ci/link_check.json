{
  "ignorePatterns": [
    {
      "pattern": "^(?!http).*"
    },
    {
      "pattern": "!\\[.*\\]\\((?!http).*\\)"
    },
    {
      "pattern": "^https:\\/\\/github\\.com\\/apache\\/hertzbeat\\/pull\\/.*"
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [
    0,
    200,
    400,
    401,
    403
  ]
}
