{
  "ignorePatterns": [
    {
      "pattern": "^(?!http).*"
    },
    {
      "pattern": "!\\[.*\\]\\((?!http).*\\)"
    },
    {
      "pattern": "^(https?://)?(hertzbeat|microsoft|dist\\.com)"
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 10,
  "fallbackRetryDelay": "1000s",
  "aliveStatusCodes": [
    0,
    200,
    401,
    403
  ]
}
